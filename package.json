{
  "name": "file-context-sidebar",
  "displayName": "File Context Sidebar",
  "description": "Sidebar that appears on right-click with file information",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:fileContextSidebar.openSidebar",
    "onCommand:fileContextSidebar.test"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
                {
                    "command": "myExtension.doSomethingWithFile",
                    "title": "Do Something with this File"
                }
            ],
            "menus": {
            "explorer/context": [
                {
                    "command": "myExtension.doSomethingWithFile",
                    "group": "navigation@1"
                }
            ]
        }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/node": "14.x",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "axios": "^0.21.4"
  }
}