{
  "name": "file-context-sidebar",
  "displayName": "AIM File Context Sidebar",
  "description": "AIM Sidebar that appears on right-click with file path to ease generation of tests with AIM",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aimPanel.openAIM",
        "title": "Открыть меню AIM"
      },
      {
        "command": "aimPanel.openSidebar",
        "title": "Открыть Sidebar AIM"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "aimPanel.openAIM",
          "group": "navigation@1"
        }
      ]
    },
    "configuration": {
      "title": "AIM - генерация автотеста по примеру",
      "properties": {
        "fileContextSidebar.apiEndpoints": {
          "type": "array",
          "default": [
            "https://jsonplaceholder.typicode.com/docs",
            "http://localhost:3000/docs"
          ],
          "description": "Список эндпоинтов API "
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/node": "14.x",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}